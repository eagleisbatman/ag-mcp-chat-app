{
  "name": "ag-mcp-api-gateway",
  "version": "3.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "npx prisma generate && npx prisma db push && node prisma/seed.js && node dist/index.js",
    "start:legacy": "npx prisma generate && npx prisma db push && node prisma/seed.js && node index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cloudinary": "^1.41.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.15",
    "prisma": "^5.22.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
